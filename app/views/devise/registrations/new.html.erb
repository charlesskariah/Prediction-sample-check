<h2>Sign up</h2>

<%= form_for(resource, class: 'form-control', as: resource_name, url: registration_path(resource_name)) do |f| %>

  <%= f.fields_for(:preferences, Preference.new) do |o| %>

  <%= render partial: 'form', locals: { f: f, o: o} %>

  <div><%= f.submit "Sign up" %></div>
  <% end %>
<% end %>

<%= render "devise/shared/links" %>
